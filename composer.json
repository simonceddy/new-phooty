{
    "name": "simoneddy/phooty",
    "description": "more phooty new era",
    "type": "project",
    "license": "MIT",
    "config": {
       "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
       "psr-4": {
          "Phooty\\": "src/current",
          "RePhooty\\": "src/refactors"
       },
       "files": [
           "src/current/functions.php"
       ]
    },
    "require": {
        "php": ">=8.0",
        "pimple/pimple": "^3.3",
        "php-ds/php-ds": "^1.3",
        "php-ai/php-ml": "^0.4.1",
        "evenement/evenement": "^3.0",
        "react/event-loop": "^1.1.1",
        "symfony/finder": "^5.2",
        "symfony/filesystem": "^5.2",
        "doctrine/cache": "^1.10",
        "illuminate/container": "^8.37",
        "illuminate/support": "^8.37"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.2",
        "phpspec/phpspec": "^7.0"
    }
}
